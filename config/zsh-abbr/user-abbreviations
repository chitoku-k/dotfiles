abbr "git amend"="git commit --amend"
abbr "git branches"="git branch -a"
abbr "git discard"="git checkout --"
abbr "git precommit"="git diff --cached"
abbr "git preserve"="git update-index --assume-unchanged --"
abbr "git remotes"="git remote -v"
abbr "git stashes"="git stash list"
abbr "git tags"="git tag"
abbr "git uncommit"="git reset --mixed HEAD~"
abbr "git unpreserve"="git update-index --no-assume-unchanged --"
abbr "git unstage"="git reset -q HEAD --"
abbr "git worktrees"="git worktree list"
abbr "sc"="systemctl"
abbr "scs"="systemctl status"
abbr "ssc"="sudo systemctl"
